name = "cloudflare-s3-proxy"
compatibility_date = "2024-04-08"
main = "index.js"

# Don't put your application key in this file!
# You should set it as a secret in the Cloudflare dashboard or at the command line:
# echo "<your secret key>" | wrangler secret put S3_SECRET_ACCESS_KEY
[vars]
S3_ACCESS_KEY_ID = "<your key id>"
S3_SECRET_ACCESS_KEY = "<your secret key>"
S3_ENDPOINT = "<your endpoint>"

